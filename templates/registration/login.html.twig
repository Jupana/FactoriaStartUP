{% extends 'base.html.twig' %}

{% block content %}
    {% if error %}
        <div>{{ error.message }}</div>
    {% endif %}
    <!--Contenido Login-->
   <div class="row  col-md-8 bgWhite">
        <h1 class="col-12 text-center">Login StartUp</h1>
        <div class="row ">
            <div class="row  ">
                <form action="{{ path('login_check') }}" method="post">
                    <div class="form-group">
                        <label for="username">Username:</label>
                        <input type="text" class="form-control" id="username" name="_username" value="{{ last_username }}">
                    </div>
                    <div class="form-group">
                        <label for="password">Password:</label>
                        <input type="password" class="form-control" id="password" name="_password">
                    </div>
                    <input type="hidden" name="_target_path" value="proyectos_equipos" />                    
                    <input class="btn btn-info" type="submit" name="login"/> 
                </form>                     
            </div>
        </div> 
</div>  
{% endblock %}

